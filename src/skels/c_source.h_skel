  @ELSE@
  FIX_UNUSED (values);
  @ENDIF@
  if (arg) {
    @IF@ check_possible_values @THEN@
    if (values) {
      found = check_possible_values(arg, values);      
    }
    if (found >= 0)
      fprintf(outfile, "%s=\"%s\" # %s\n", opt, arg, values[found]);
    else
      fprintf(outfile, "%s=\"%s\"\n", opt, arg);
    @ELSE@
    fprintf(outfile, "%s=\"%s\"\n", opt, arg);
    @ENDIF@
  } else {
    fprintf(outfile, "%s\n", opt);
  }
}

