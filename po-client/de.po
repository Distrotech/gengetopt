msgstr "%s: Option %s kollidiert mit Option %s\n"

#: src/skels/c_source.h_skel:1552
#, c-format
msgid "%s%s: option unknown: %c\n"
msgstr "%s%s: Unbekannte Option: %c\n"

#: src/skels/c_source.h_skel:1641
#, c-format
msgid "%s: Error opening configuration file '%s'\n"
msgstr "%s: Fehler beim Öffnen der Konfigurationsdatei '%s'\n"

#: src/skels/c_source.h_skel:1659
#, c-format
msgid "%s%s: Line too long in configuration file\n"
msgstr "%s%s: Zeile in Konfigurationsdatei zu lang\n"

#: src/skels/c_source.h_skel:1710
#, c-format
msgid "%s%s: unterminated string in configuration file\n"
msgstr "%s%s: Nichtterminierte Zeichenkette in Konfigurationsdatei\n"

#: src/skels/c_source.h_skel:1733
#, c-format
msgid "%s%s: malformed string in configuration file\n"
msgstr "%s%s: Ungültig formatierte Zeichenkette in Konfigurationsdatei\n"

#: src/skels/c_source.h_skel:1747
#, c-format
msgid "%s%s: include requires a filename argument.\n"
msgstr "%s%s: 'include' braucht einen Dateinamen als Argument.\n"

#: src/skels/custom_getopt_gen.h_skel:328
#, c-format
msgid "%s: option `%s' is ambiguous\n"
msgstr "%s: Option `%s' is uneindeutig\n"

#: src/skels/custom_getopt_gen.h_skel:346
#, c-format
msgid "%s: option `--%s' doesn't allow an argument\n"
msgstr "%s: Option `--%s' erlaubt kein Argument\n"

#: src/skels/custom_getopt_gen.h_skel:350
#, c-format
msgid "%s: option `%c%s' doesn't allow an argument\n"
msgstr "%s: Option `%c%s' erlaubt keinen Parameter\n"

#: src/skels/custom_getopt_gen.h_skel:365
#, c-format
msgid "%s: option `%s' requires an argument\n"
msgstr "%s: Option `%s' benötigt einen Parameter\n"

#: src/skels/custom_getopt_gen.h_skel:392
#, c-format
msgid "%s: unrecognized option `--%s'\n"
msgstr "%s: Unbekannte Option `--%s'\n"

#: src/skels/custom_getopt_gen.h_skel:397
#, c-format
msgid "%s: unrecognized option `%c%s'\n"
msgstr "%s: Unbekannte Option `%c%s'\n"

#: src/skels/custom_getopt_gen.h_skel:419
#, c-format
msgid "%s: invalid option -- %c\n"
msgstr "%s: Ungültige Option -- %c\n"

#: src/skels/custom_getopt_gen.h_skel:446
#, c-format
msgid "%s: option requires an argument -- %c\n"
msgstr "%s: Option benötigt einen Parameter -- %c\n"

#: src/skels/dependant_option.h_skel:3
#, c-format
msgid "%s%s: %s option depends on option '%s'\n"
msgstr "%s%s: %s-Option hängt ab von Option '%s'\n"

#: src/skels/group_option.h_skel:4
msgid ""
"%s%s: one option of group %$3s was given.  The number required is %$4d.\n"
msgid_plural ""
"%s%s: %$5d options of group %$3s were given.  The number required is %$4d.\n"
msgstr[0] ""
"%s%s: Eine Option der Gruppe %$3s wurde angegeben. Es werden %$4d benötigt.\n"
msgstr[1] ""
"%s%s: %$5d Optionen der Gruppe %$3s wurden angegeben. Es werden %$4d "
"benötigt.\n"

#: src/skels/required_option.h_skel:8
#, c-format
msgid "%s%s: %s option required\n"
msgstr "%s%s: %s Option erforderlich\n"

