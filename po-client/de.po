#: src/skels/c_source.h_skel:687
#, c-format
msgid "%s: cannot dump options to stream\n"
msgstr "%s: Kann Optionen nicht in Stream schreiben\n"

#: src/skels/c_source.h_skel:707
#, c-format
msgid "%s: cannot open file for writing: %s\n"
msgstr "%s: Kann Datei nicht zum Schreiben öffnen: %s\n"

#: src/skels/c_source.h_skel:841
#, c-format
msgid "%s: %s option must occur once\n"
msgid_plural "%s: %s option must occur %d times\n"
msgstr[0] "%s: %s Option muss einmal vorkommen\n"
msgstr[1] "%s: %s Option muss %d-mal vorkommen\n"

#: src/skels/c_source.h_skel:852
#, c-format
msgid "%s: %s option must occur between %d and %d times\n"
msgstr "%s: %s Option muss zwischen %d- und %d-mal vorkommen\n"

#: src/skels/c_source.h_skel:863
#, c-format
msgid "%s: %s option must occur at least once\n"
msgid_plural "%s: %s option must occur at least %d times\n"
msgstr[0] "%s: %s Option muss mindestens einmal vorkommen\n"
msgstr[1] "%s: %s Option muss mindestens %d-mal vorkommen\n"

#: src/skels/c_source.h_skel:875
#, c-format
msgid "%s: %s option must occur at most once\n"
msgid_plural "%s: %s option must occur at most %d times\n"
msgstr[0] "%s: %s Option darf nur einmal vorkommen\n"
msgstr[1] "%s: %s Option darf maximal %d-mal vorkommen\n"

#: src/skels/c_source.h_skel:1003
#, c-format
msgid "%s%s: `--%s' (`-%c') option given more than once\n"
msgstr "%s%s: `--%s' (`-%c') Option mehr als einmal angegeben\n"

#: src/skels/c_source.h_skel:1007
#, c-format
msgid "%s%s: `--%s' option given more than once\n"
msgstr "%s%s: `--%s'Option mehr als einmal angegeben\n"

#: src/skels/c_source.h_skel:1018
#, c-format
msgid "%s%s: ambiguous argument, \"%s\", for option `--%s' (`-%c')\n"
msgstr "%s%s: Uneindeutiges Argument \"%s\" für Option `--%s' (`-%c')\n"

#: src/skels/c_source.h_skel:1022
#, c-format
msgid "%s%s: ambiguous argument, \"%s\", for option `--%s'\n"
msgstr "%s%s: Uneindeutiges Argument \"%s\" für Option `--%s'\n"

#: src/skels/c_source.h_skel:1027
#, c-format
msgid "%s%s: invalid argument, \"%s\", for option `--%s' (`-%c')\n"
msgstr "%s%s: Ungültiges Argument \"%s\" für Option `--%s' (`-%c')\n"

#: src/skels/c_source.h_skel:1031
#, c-format
msgid "%s%s: invalid argument, \"%s\", for option `--%s'\n"
