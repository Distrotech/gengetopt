        if (argv[optind++] != argv[0])
          args_info->inputs[ i++ ] = gengetopt_strdup (argv[optind-1]) ;
    }

  return 0;

failure:
  
  default_values_cmd_parser_release (&local_args_info);
  return (EXIT_FAILURE);
}
