  char *multi_token, *multi_next; /* for multiple options */

  int i;        /* Counter */

  struct int_list * multi_list = NULL,* multi_new = NULL;
  int error = 0;
  struct gengetopt_args_info local_args_info;

  if (initialize)
    no_multi_strings_cmd_parser_init (args_info);

  no_multi_strings_cmd_parser_init (&local_args_info);

  optarg = 0;
  optind = 0;
  opterr = 1;
  optopt = '?';

  while (1)
    {
      int option_index = 0;
      char *stop_char;

      static struct option long_options[] = {
        { "help",	0, NULL, 'h' },
        { "version",	0, NULL, 'V' },
        { "test",	1, NULL, 't' },
        { "multi",	1, NULL, 0 },
        { NULL,	0, NULL, 0 }
      };

      stop_char = 0;
      c = getopt_long (argc, argv, "hVt:", long_options, &option_index);

      if (c == -1) break;	/* Exit from `while (1)' loop.  */

      switch (c)
        {
        case 'h':	/* Print help and exit.  */
          no_multi_strings_cmd_parser_print_help ();
          no_multi_strings_cmd_parser_free (&local_args_info);
          exit (EXIT_SUCCESS);

        case 'V':	/* Print version and exit.  */
          no_multi_strings_cmd_parser_print_version ();
          no_multi_strings_cmd_parser_free (&local_args_info);
          exit (EXIT_SUCCESS);

        case 't':	/* This is a test option.  */
          if (local_args_info.test_given)
            {
              fprintf (stderr, "%s: `--test' (`-t') option given more than once%s\n", argv[0], (additional_error ? additional_error : ""));
              goto failure;
            }
          if (args_info->test_given && ! override)
            continue;
          local_args_info.test_given = 1;
          args_info->test_given = 1;
          args_info->test_arg = strtol (optarg, &stop_char, 0);
          if (!(stop_char && *stop_char == '\0')) {
            fprintf(stderr, "%s: invalid numeric value: %s\n", argv[0], optarg);
            goto failure;
          }
          if (args_info->test_orig)
            free (args_info->test_orig); /* free previous string */
          args_info->test_orig = gengetopt_strdup (optarg);
          break;


        case 0:	/* Long option with no short option */
          /* This is a multi option.  */
          if (strcmp (long_options[option_index].name, "multi") == 0)
          {
            local_args_info.multi_given++;
          
            multi_token = get_multiple_arg_token(optarg);
            multi_next = get_multiple_arg_token_next (optarg);
          
            while (1)
              {
                multi_new = (struct int_list *) malloc (sizeof (struct int_list));
                multi_new->next = multi_list;
                multi_list = multi_new;
                multi_new->arg = strtol (multi_token, &stop_char, 0);
                if (!(stop_char && *stop_char == '\0')) {
                  fprintf(stderr, "%s: invalid numeric value: %s\n", argv[0], multi_token);
                  goto failure;
                }
                multi_new->orig = multi_token;
          
                if (multi_next)
                  {
                    multi_token = get_multiple_arg_token(multi_next);
                    multi_next = get_multiple_arg_token_next (multi_next);
                    local_args_info.multi_given++;
                  }
                else
                  break;
              }
            break;
          }
          
          break;
        case '?':	/* Invalid option.  */
          /* `getopt_long' already printed an error message.  */
          goto failure;

        default:	/* bug: option not considered.  */
          fprintf (stderr, "%s: option unknown: %c%s\n", NO_MULTI_STRINGS_CMD_PARSER_PACKAGE, c, (additional_error ? additional_error : ""));
          abort ();
        } /* switch */
    } /* while */


  if (local_args_info.multi_given && multi_list)
    {
      struct int_list *tmp;
      args_info->multi_arg = (int *) realloc (args_info->multi_arg, (args_info->multi_given + local_args_info.multi_given) * sizeof (int));
      args_info->multi_orig = (char **) realloc (args_info->multi_orig, (args_info->multi_given + local_args_info.multi_given) * sizeof (char *));
      for (i = (local_args_info.multi_given - 1); i >= 0; --i)
        {
          tmp = multi_list;
          args_info->multi_arg [i + args_info->multi_given] = multi_list->arg;
          args_info->multi_orig [i + args_info->multi_given] = multi_list->orig;
          multi_list = multi_list->next;
          free (tmp);
        }
    }
  

  args_info->multi_given += local_args_info.multi_given;
  local_args_info.multi_given = 0;
  
  if (check_required)
    {
      error += no_multi_strings_cmd_parser_required2 (args_info, argv[0], additional_error);
    }

  no_multi_strings_cmd_parser_release (&local_args_info);

  if ( error )
    return (EXIT_FAILURE);

  if (optind < argc)
    {
      int i = 0 ;
      int found_prog_name = 0;
      /* whether program name, i.e., argv[0], is in the remaining args
         (this may happen with some implementations of getopt,
          but surely not with the one included by gengetopt) */

      i = optind;
      while (i < argc)
        if (argv[i++] == argv[0]) {
          found_prog_name = 1;
          break;
        }
      i = 0;

      args_info->inputs_num = argc - optind - found_prog_name;
      args_info->inputs =
        (char **)(malloc ((args_info->inputs_num)*sizeof(char *))) ;
      while (optind < argc)
        if (argv[optind++] != argv[0])
          args_info->inputs[ i++ ] = gengetopt_strdup (argv[optind-1]) ;
    }

  return 0;

failure:
  if (multi_list)
    {
      struct int_list *tmp;
      while (multi_list)
        {
          tmp = multi_list;
          free (multi_list->orig);
          multi_list = multi_list->next;
          free (tmp);
        }
    }
  
  no_multi_strings_cmd_parser_release (&local_args_info);
  return (EXIT_FAILURE);
}
